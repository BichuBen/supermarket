<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="crossbrowsertesting" thread-count="4" parallel="tests">
	<listeners>
		<listener class-name="com.supermarket.listeners.ExtentReporter"></listener>
	</listeners>
	
	<test name="browser chrome">
		<parameter name="browser" value="chrome"></parameter>

		<classes>
			<class name="com.supermarket.tests.AdminUserstest"></class>
			<class name="com.supermarket.tests.Home_PageTests"></class>
		</classes>

	</test>

	<test name="browser edge">
		<parameter name="browser" value="edge"></parameter>

		<classes>
			<class name="com.supermarket.tests.AdminUserstest"></class>
			<class name="com.supermarket.tests.Home_PageTests"></class>
		</classes>

	</test>
</suite>